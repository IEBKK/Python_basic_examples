   "source": [
    "in_file = open('chicken.txt', 'r')\n",
    "\n",
    "day = 0\n",
    "income = 0\n",
    "for line in in_file:\n",
    "    data = line.split()\n",
    "    day += 1\n",
    "    income += int(data[1])\n",
    "in_file.close()\n",
    "\n",
    "average = income / day\n",
    "print(average)"
